{
  "name": "toy-office",
  "version": "1.0.0",
  "main": "./lib/toy-sheet.umd.js",
  "module": "./lib/toy-sheet.es.js",
  "exports": {
    ".": {
      "import": "./lib/toy-sheet.es.js",
      "require": "./lib/toy-sheet.umd.js"
    }
